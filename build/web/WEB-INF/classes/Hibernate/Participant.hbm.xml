<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="Model.Participant" table="participant">
    <id name="participantId" column="participantId"></id>
    <property name="eventId" column="eventId" />
    <property name="userEmail" column="userEmail" />
    <property name="isOrganizer" column="isOrganizer" />
    <property name="isParticipant" column="isParticipant"/>
    <property name="isGuest" column="isGuest"/>
    <property name="isAttending" column="isAttending" />
</class>
</hibernate-mapping>